UPSERT INTO TABLE ${var:db_schema}.${var:db_weather_table_kudu} SELECT _timestamp, CAST(longitude as STRING) as longitude_pk, CAST(latitude as STRING) as latitude_pk, ST_SETSRID(ST_POINT(longitude, latitude), 4326) AS geom, longitude, latitude, round(u_wind,2), round(v_wind,2), round(wave_height,2), wave_direction, round(wave_period,2), round(swell_height,2), swell_direction, round(swell_period,2), ice_cover, round(u_current,2), round(v_current,2), round(water_temperature,1), round(pressure,2), round(air_temperature,1) FROM ${var:db_schema}.${var:db_weather_table_temp}
